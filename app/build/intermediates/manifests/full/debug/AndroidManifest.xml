<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.listforschool"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.user.listforschool.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.user.listforschool.StudentActivity"
            android:parentActivityName="com.example.user.listforschool.MainActivity" >
            <meta-data
                android:name="android.support.PARRENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name="com.example.user.listforschool.EditStudentActivity"
            android:parentActivityName="com.example.user.listforschool.StudentActivity" >
            <meta-data
                android:name="android.support.PARRENT_ACTIVITY"
                android:value=".StudentActivity" />
        </activity>
    </application>

</manifest>